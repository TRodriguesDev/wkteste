unit uControllerItemPedido;

interface

uses uMItemPedido, SysUtils;

Type
  TControllerItemPedido = class
  private
    fMItemPedido: TMItemPedido;

    public
      property MPedido : TMItemPedido read fMItemPedido write fMItemPedido;

      constructor Create;
      destructor destroy; override;
  end;

implementation

{ TControllerPedido }

constructor TControllerItemPedido.Create;
begin
  fMItemPedido := TMItemPedido.create;
  inherited Create;
end;

destructor TControllerItemPedido.destroy;
begin
  Freeandnil(FMItemPedido);
  inherited;
end;


end.
